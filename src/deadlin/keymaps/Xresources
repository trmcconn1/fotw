Ctrl<Key>W: string("W")\n\
Ctrl<Key>R: string("R")\n\
Ctrl<Key>L: string("L")\n\
Ctrl<Key>T: string("T")\n\
Ctrl<Key>U: string("U")\n\
Ctrl<Key>E: string("E")\n\
